<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" fx:id="ap" prefHeight="400.0" prefWidth="600.0" styleClass="Labels" stylesheets="@../styles/style.css" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ch.bbbaden.casino.loginregister.LoginController">
   <children>
      <TextField fx:id="namefield" layoutX="219.0" layoutY="99.0" promptText="username" />
      <PasswordField fx:id="passwordfield" layoutX="219.0" layoutY="200.0" promptText="password" />
      <Label fx:id="title" layoutX="277.0" layoutY="14.0" text="Login" />
      <Label fx:id="namelabel" layoutX="266.0" layoutY="75.0" text="Username" />
      <Label fx:id="passwordlabel" layoutX="268.0" layoutY="174.0" text="Password" />
      <Button fx:id="login" layoutX="268.0" layoutY="282.0" mnemonicParsing="false" onAction="#login" text="login" />
      <Label fx:id="alertname" layoutX="218.0" layoutY="131.0" prefHeight="18.0" prefWidth="149.0" styleClass="Labels" stylesheets="@../styles/style.css" textAlignment="CENTER" textFill="RED" />
      <Label fx:id="alertpassword" layoutX="218.0" layoutY="237.0" prefHeight="18.0" prefWidth="149.0" />
      <Label fx:id="alertcheck" layoutX="216.0" layoutY="255.0" prefHeight="18.0" prefWidth="150.0" textAlignment="CENTER" textFill="#2ddd06" />
      <Label fx:id="textlabel" layoutX="210.0" layoutY="324.0" prefHeight="18.0" prefWidth="205.0" text="You don't have an Account?" />
      <Label fx:id="registerlabel" layoutX="246.0" layoutY="355.0" onMouseClicked="#register" prefHeight="18.0" prefWidth="142.0" text="Create one here!" textFill="#1d00f8" />
   </children>
</AnchorPane>
